<launch>
  <env name="ROSCONSOLE_CONFIG_FILE" value="$(find supereight_ros)/config/rosconsole.conf"/>
  <env name="ROS_IP" value="192.168.1.183"/> <!-- PC on WiFi -->
  <env name="ROS_MASTER_URI" value="http://192.168.1.120:11311/"/> <!-- MAV on WiFi -->
  <!-- <env name="ROS_MASTER_URI" value="http://192.168.1.204:11311/"/> --> <!-- MAV on Ethernet -->

  <node pkg="rviz" type="rviz" name="rviz" required="true"
        args="--display-config $(find supereight_ros)/config/experiment.rviz --disable-anti-aliasing --splash-screen .nonexistent"/>
</launch>
