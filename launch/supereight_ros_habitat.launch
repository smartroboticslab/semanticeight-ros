<launch>
  <include file="$(find supereight_ros)/launch/supereight_ros.launch">
    <!-- Use a different config file -->
    <arg name="supereight_ros_config_file"
         value="$(find supereight_ros)/config/config_habitat.yaml"/>
    <!-- Override topic names and types -->
    <arg name="depth_image_topic" value="/habitat/depth/image_raw"/>
    <arg name="rgb_image_topic"   value="/habitat/rgb/image_raw"/>
    <arg name="pose_topic"        value="/habitat/pose"/>
  </include>
</launch>

