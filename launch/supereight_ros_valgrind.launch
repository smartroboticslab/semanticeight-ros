<launch>
  <include file="$(find supereight_ros)/launch/supereight_ros.launch">
    <!-- Override default node argument values -->
    <arg name="command_prefix"
         value="valgrind --track-origins=yes --leak-check=full --read-var-info=yes --leak-check=full --leak-resolution=high"/>
  </include>
</launch>

